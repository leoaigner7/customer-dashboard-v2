{
  "updateApi": "https://api.github.com/repos/leoaigner7/customer-dashboard-v2/releases/latest",

  "mode": "auto-install",
  "checkIntervalMs": 60000,

  "target": {
    "type": "docker-dashboard",
    "envFile": "../deploy/.env",
    "versionKey": "APP_VERSION",
    "composeFile": "../deploy/docker-compose.yml",
    "serviceName": "dashboard"
  },

  "artifacts": {
    "type": "docker-image",
    "imageTemplate": "ghcr.io/leoaigner7/customer-dashboard-v2:{version}"
  },

  "notification": {
  "logFile": "../../deploy/system-daemon.log"
  }
}
